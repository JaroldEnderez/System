{
  "name": "@dhtmlx/trial-react-gantt",
  "deployTag": "react-gantt",
  "productTag": "gantt",
  "version": "1.1.4",
  "main": "public/dist/gantt.cjs.js",
  "module": "public/dist/gantt.es.js",
  "source": "src/main.js",
  "demos": "https://dhtmlx.com/react/demos/gantt",
  "scripts": {
    "build": "rollup --config-production -c",
    "build:trial": "rollup --config-production  --config-limited -c",
    "build:demos": "rollup --config-demos --config-production --config-limited -c",
    "build:site": "rollup --config-site --config-production --config-limited -c",
    "watch": "rollup -c -w",
    "watch:demos": "rollup --config-demos -c -w",
    "watch:site": "rollup --config-site -c -w",
    "lint": "yarn eslint ./demos ./src ./site --ext .jsx,.ts,.js",
    "dev": "run-p start:dev watch:demos",
    "dev:site": "run-p start:site watch:site",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "start:site": "sirv public/site --dev",
    "prepare": "husky install",
    "test": "./bin/run-tests.sh"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@dhtmlx/gantt-data-provider": "^0.2.0",
    "@dhtmlx/trial-lib-gantt": "^1.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.17.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.0.0-0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.39.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "sirv-cli": "^1.0.11"
  },
  "files": [
    "public/dist",
    "src"
  ]
}
